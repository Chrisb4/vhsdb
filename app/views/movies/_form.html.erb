
<%= form_for @movie do |f| %>
<p>  <%= f.label :title %>
    <%= f.text_field :title %>
</p>
<p>  <%= f.label :summary %>
    <%= f.text_field :summary %>
</p>
<p>  <%= f.label :youtube %>
    <%= f.text_field :youtube %>
</p>
<p>  <%= f.label :thumbnail %>
    <%= f.text_field :thumbnail%>
</p>


<%= f.submit %> or <%= link_to 'Cancel', movies_path %>

<% end %>
